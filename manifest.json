{
    "manifest_version":2,
    "version":"1.1.1.1",
    "name": "MyScript",
    "content_scripts": [
        {
            "js": ["content.js"],
            "matches": ["https://example.net/*"]
        }
    ],
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "page_action": {
        "default_icon": "ext_on.png",
        "default_title": "MyScript"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "storage"
    ]
}
